{
  "name": "todo-app",
  "version": "1.0.0",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kfedechkin/fdk-eslint-config": "1.0.13",
    "@parcel/transformer-svg-react": "^2.7.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.28.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "parcel": "^2.7.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.2"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "fdk-eslint-config"
    ]
  },
  "alias": {
    "common": "./src/common/",
    "hooks": "./src/hooks/"
  }
}
